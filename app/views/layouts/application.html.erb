<!DOCTYPE html>
<html>
  <head>
    <title>AdminLte3</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'adminlte.css' %>
    <%= stylesheet_link_tag 'adminlte.min.css' %>
    <%= stylesheet_link_tag 'ionicons.min.css' %>
    <%= stylesheet_link_tag 'tempusdominus-bootstrap-4.min.css' %>
    <%= stylesheet_link_tag 'OverlayScrollbars.min.css' %>
    <%= stylesheet_link_tag 'daterangepicker.css' %>
    <%= stylesheet_link_tag 'summernote-bs4.css' %>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    

    
    
    
    
  






  </head>

  <body data-scrollbar-auto-hide="n" class="hold-transition sidebar-mini layout-fixed">
      <div class="wrapper">
        <%= render "base/header" %>
        <%= render "base/sidebar" %>


          <div class="content container-fluid">
            <div class="container-fluid">
              <%= yield %>
            </div>
          </div>
        </div>

        <%= render "base/footer" %>
        <%= render "base/control_sidebar" %>
      <div>
  </body>
</html>