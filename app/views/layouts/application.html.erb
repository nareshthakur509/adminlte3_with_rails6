  <body data-scrollbar-auto-hide="n" class="hold-transition sidebar-mini layout-fixed">
      <div class="wrapper">
        <%= render "base/header" %>
        <%= render "base/sidebar" %>
        

        <div>
          <%
=begin%>
 <div class="preloader flex-column justify-content-center align-items-center">
             <img class="animation__wobble" src="/assets/AdminLTELogo.png" alt="AdminLTELogo" height="50" width="50">
          </div> 
<%
=end%>
           <%= yield %>
        </div>
        <%= render "base/footer" %>
        <%= render "base/control_sidebar" %>
      <div>
  </body>
</html>